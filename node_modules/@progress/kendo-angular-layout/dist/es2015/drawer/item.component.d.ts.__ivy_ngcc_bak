import { TemplateRef, ElementRef, Renderer2 } from "@angular/core";
import { DrawerService } from './drawer.service';
/**
 * @hidden
 */
export declare class DrawerItemComponent {
    drawer: DrawerService;
    private element;
    private renderer;
    item: any;
    index: number;
    itemTemplate: TemplateRef<any>;
    mini: boolean;
    expanded: boolean;
    disabled: boolean;
    cssClass: boolean;
    cssStyle: boolean;
    readonly disabledClass: boolean;
    readonly selectedClass: boolean;
    readonly label: boolean;
    constructor(drawer: DrawerService, element: ElementRef, renderer: Renderer2);
    /**
     * @hidden
     */
    ngAfterViewInit(): void;
    /**
     * @hidden
     */
    iconClasses(icon?: string): string;
}
